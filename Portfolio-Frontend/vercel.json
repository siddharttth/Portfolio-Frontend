{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node-builder",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://portfolio-backend-01.up.railway.app"
  }
}
